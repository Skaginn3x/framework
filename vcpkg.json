{
  "name": "tfc-framework",
  "version": "2023.11.1",
  "dependencies": [
    "async-mqtt",
    "azmq",
    "bext-sml",
    "bext-ut",
    "boost-asio",
    "boost-program-options",
    "boost-stacktrace",
    "boost-lexical-cast",
    "date",
    "fmt",
    {
      "name": "glaze",
      "version>=": "1.8.4"
    },
    "gtest",
    {
      "name": "libgpiod",
      "features": [
        "cxx-bindings"
      ]
    },
    "magic-enum",
    {
      "name": "mp-units",
      "features": [
        "use-libfmt"
      ]
    },
    "openssl",
    "pcg",
    "protobuf",
    {
      "name": "sdbusplus",
      "version>=": "2023-09-14",
      "features": [
        "asio-only"
      ]
    },
    "soem",
    "spdlog",
    "stduuid",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "build-exes": {
      "description": "Build binaries and install them to sysroot"
    }
  }
}
