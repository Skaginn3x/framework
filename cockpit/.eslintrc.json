{
   "extends": [
      "react-app",
      "airbnb",
      "airbnb-typescript"
   ],
   "parserOptions": {
      "project": "./tsconfig.json"
   },
   "rules": {
      "max-len": [
         "error",
         {
            "code": 140
         }
      ],
      "react-hooks/exhaustive-deps": "off",
      "no-console": "off",
      "spaced-comment": "warn"
   }
}